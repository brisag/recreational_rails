<header>
  <nav>
    <%= link_to "All Parks", "/parks" %>
    <%= link_to "All Programs", "/programs" %>
  </nav>
</header>

<h1>All Programs</h1>

<section id="activity">
<% @programs.each do |program| %>
    <h2>Name: <%= program.name %></h2>
    <p>Number of Participants: <%= program.num_of_participants %></p>
    <p>Virtual Program? <%= program.virtual %></p> 
<% end %>
</section>
