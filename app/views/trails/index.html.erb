<header>
  <nav>
    <%= link_to "All Backcountry Offices", "/offices" %>
    <%= link_to "All Trails", "/trails" %>
  </nav>
</header>

<section id="trails">
  <% @trails.each do |trail| %>
  <h2><%= trail.name %></h2>
  <p>Elevation Gain (feet): <%= trail.elevation %></p>
  <p>Dogs Allowed? <%= trail.dogs_allowed %></p>
  <% end %>
</section>
